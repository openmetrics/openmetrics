<h3>IpLookup <small>#<%= @ip_lookup.id %></small></h3>
Target: <%= @ip_lookup.target %><br>
Text result:<br>
<%= coderay(@ip_lookup.ip_lookup_result.result, 'json', 'div') %>

<% if @ip_lookup.result %>
<h3>Result</h3>
<% json_result = @ip_lookup.result.is_json? ? JSON.parse(@ip_lookup.result) : false %>
<% if json_result %>
    <% json_result.each do |node| %>
            <h4><%= node['addr'] %></h4>
            <%= node['port']['service']['name'] %> (<%= node['port']['service']['product'] %> <%= node['port']['service']['version'] %>)
        <% end %>
    <% end %>
<% else %>
    <%= @ip_lookup.result %>
<% end %>





