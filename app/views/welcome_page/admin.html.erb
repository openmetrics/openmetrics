<h3>Admin Area</h3>

<h4 class="spacer">Environment Variables</h4>
<% ENV.each_pair do |name, value| %>
    <% if name.starts_with? 'OM_' %>
        <%= name %>: <%= value %><br>
    <% end %>
<% end %>


<h4 class="spacer">Sidekiq Background Processor</h4>
<div class="panel panel-default">

  <div class="panel-body">
    <p>Click <%= link_to "here", '/sidekiq', :target => :blank %> to open sidekiq UI in full size.</p>
    <iframe src="/sidekiq" width="100%" height="460">
    </iframe>
  </div>

</div>


